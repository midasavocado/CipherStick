{
  "shortcuts": [
    {
      "id": "spotify-to-apple-music",
      "name": "Spotify to Apple Music",
      "description": "Convert playlists instantly between Spotify and Apple Music. Supports public and private playlists.",
      "author": {
        "name": "Alex Chen",
        "username": "alex",
        "avatarColor": "linear-gradient(135deg, #3b82f6, #8b5cf6)",
        "bio": "Full-stack developer and automation enthusiast. I love creating shortcuts that save time.",
        "avatarUrl": "https://i.pravatar.cc/150?u=alex"
      },
      "icon": "music",
      "iconColor": "#8b5cf6",
      "likes": 420,
      "downloads": 1200,
      "views": 2800,
      "createdAt": "2024-12-15",
      "actions": [
        { "name": "Get Clipboard", "icon": "clipboard", "color": "gray", "params": "" },
        { "name": "Download URL", "icon": "download", "color": "green", "params": "Spotify API" },
        {
          "name": "If",
          "icon": "branch",
          "color": "purple",
          "params": "URL contains \"spotify\"",
          "then": [
            { "name": "Get Dictionary Value", "icon": "dict", "color": "orange", "params": "Key: tracks" },
            {
              "name": "Repeat with Each",
              "icon": "repeat",
              "color": "purple",
              "params": "Track in playlist",
              "children": [
                { "name": "Search Apple Music", "icon": "search", "color": "pink", "params": "Query: track name" },
                { "name": "Add to Playlist", "icon": "plus", "color": "green", "params": "My Converted Playlist" }
              ]
            }
          ],
          "else": [
            { "name": "Show Alert", "icon": "alert", "color": "red", "params": "\"Invalid Spotify URL\"" }
          ]
        },
        { "name": "Show Result", "icon": "check", "color": "green", "params": "Playlist converted!" }
      ]
    },
